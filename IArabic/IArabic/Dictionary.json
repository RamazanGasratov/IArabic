    "rusText": "Из, от",
    "arText": "مِنْ"
  },
  {
    "id": 2,
    "rusText": "Бог",
    "arText": "اللَّهُ"
  },
  {
    "id": 3,
    "rusText": "Нет",
    "arText": "لَا"
  },
  {
    "id": 4,
    "rusText": "В",
    "arText": "فِي"
  },
  {
    "id": 5,
    "rusText": "Истинно, действительно",
    "arText": "إِنَّ"
  },
  {
    "id": 6,
    "rusText": "Сказали",
    "arText": "قَالَ"
  },
  {
    "id": 7,
    "rusText": "Который",
    "arText": "الَّذِي"
  },
  {
    "id": 8,
    "rusText": "На, над",
    "arText": "عَلَى"
  },
  {
    "id": 9,
    "rusText": "Безусловно",
    "arText": "لَا"
  },
  {
    "id": 10,
    "rusText": "Быть, существовать",
    "arText": "كَانَ"
  },
  {
    "id": 11,
    "rusText": "Что?",
    "arText": "مَا"
  },
  {
    "id": 12,
    "rusText": "Господь",
    "arText": "رَبُّ"
  },
  {
    "id": 13,
    "rusText": "Кто?",
    "arText": "مَنْ"
  },
  {
    "id": 14,
    "rusText": "В, на",
    "arText": "بِ"
  },
  {
    "id": 15,
    "rusText": "К, на",
    "arText": "إِلَى"
  },
  {
    "id": 16,
    "rusText": "Если",
    "arText": "إِنْ"
  },
  {
    "id": 17,
    "rusText": "Кроме",
    "arText": "إِلَّا"
  },
  {
    "id": 18,
    "rusText": "Что, чтобы",
    "arText": "لِ"
  },
  {
    "id": 19,
    "rusText": "Уверовать",
    "arText": "آمَنَ"
  },
  {
    "id": 20,
    "rusText": "Тот",
    "arText": "ذَلِكَ"
  },
  {
    "id": 21,
    "rusText": "От",
    "arText": "عَنْ"
  },
  {
    "id": 22,
    "rusText": "Он",
    "arText": "هُوَ"
  },
  {
    "id": 23,
    "rusText": "Земля",
    "arText": "أَرْضٌ"
  },
  {
    "id": 24,
    "rusText": "Когда, если",
    "arText": "إِذَا"
  },
  {
    "id": 25,
    "rusText": "Уже (перед гл. прош. вр), иногда, вероятно (перед гл. наст.-будущ. вр)",
    "arText": "قَدْ"
  },
  {
    "id": 26,
    "rusText": "День",
    "arText": "يَوْمٌ"
  },
  {
    "id": 27,
    "rusText": "Народ",
    "arText": "قَوْمٌ"
  },
  {
    "id": 28,
    "rusText": "Знамение",
    "arText": "آيَةٌ"
  },
