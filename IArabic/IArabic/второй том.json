

[
    {
        "id": 1,
        "rusText": "Из, от",
        "arText": "مِنْ"
    },
    {
        "id": 2,
        "rusText": "Бог",
        "arText": "اللَّهُ"
    },
    {
        "id": 3,
        "rusText": "Нет",
        "arText": "لَا"
    },
    {
        "id": 4,
        "rusText": "В",
        "arText": "فِي"
    },
    {
        "id": 5,
        "rusText": "Истинно, действительно",
        "arText": "إِنَّ"
    },
    {
        "id": 6,
        "rusText": "Сказали",
        "arText": "قَالَ"
    },
    {
        "id": 7,
        "rusText": "Который",
        "arText": "الَّذِي"
    },
    {
        "id": 8,
        "rusText": "На, над",
        "arText": "عَلَى"
    },
    {
        "id": 9,
        "rusText": "Безусловно",
        "arText": "لَا"
    },
    {
        "id": 10,
        "rusText": "Быть, существовать",
        "arText": "كَانَ"
    },
    {
        "id": 11,
        "rusText": "Что?",
        "arText": "مَا"
    },
    {
        "id": 12,
        "rusText": "Господь",
        "arText": "رَبُّ"
    },
    {
        "id": 13,
        "rusText": "Кто?",
        "arText": "مَنْ"
    },
    {
        "id": 14,
        "rusText": "В, на",
        "arText": "بِ"
    },
    {
        "id": 15,
        "rusText": "К, на",
        "arText": "إِلَى"
    },
    {
        "id": 16,
        "rusText": "Если",
        "arText": "إِنْ"
    },
    {
        "id": 17,
        "rusText": "Кроме",
        "arText": "إِلَّا"
    },
    {
        "id": 18,
        "rusText": "Что, чтобы",
        "arText": "لِ"
    },
    {
        "id": 19,
        "rusText": "Уверовать",
        "arText": "آمَنَ"
    },
    {
        "id": 20,
        "rusText": "Тот",
        "arText": "ذَلِكَ"
    },
    {
        "id": 21,
        "rusText": "От",
        "arText": "عَنْ"
    },
    {
        "id": 22,
        "rusText": "Он",
        "arText": "هُوَ"
    },
    {
        "id": 23,
        "rusText": "Земля",
        "arText": "أَرْضٌ"
    },
    {
        "id": 24,
        "rusText": "Когда, если",
        "arText": "إِذَا"
    },
    {
        "id": 25,
        "rusText": "Уже (перед гл. прош. вр), иногда, вероятно (перед гл. наст.-будущ. вр)",
        "arText": "قَدْ"
    },
    {
        "id": 26,
        "rusText": "День",
        "arText": "يَوْمٌ"
    },
    {
        "id": 27,
        "rusText": "Народ",
        "arText": "قَوْمٌ"
    },
    {
        "id": 28,
        "rusText": "Знамение",
        "arText": "آيَةٌ"
    },
    {
        "id": 30,
        "rusText": "Идти, проходить",
        "arText": "يَمُرُّ"
    },
    {
        "id": 31,
        "rusText": "Книга",
        "arText": "كِتَابٌ"
    },
    {
        "id": 32,
        "rusText": "Истина",
        "arText": "حَقٌّ"
    },
    {
        "id": 33,
        "rusText": "До",
        "arText": "قَبْلَ"
    },
    {
        "id": 34,
        "rusText": "Люди",
        "arText": "نَاسٌ"
    },
    {
        "id": 35,
        "rusText": "Когда, так как",
        "arText": "عِنْدَ"
    },
    {
        "id": 36,
        "rusText": "Желать, хотеть",
        "arText": "أَرَادَ"
    },
    {
        "id": 37,
        "rusText": "Те",
        "arText": "أُولَئِكَ"
    },
    {
        "id": 38,
        "rusText": "Верующий",
        "arText": "مُؤْمِنٌ"
    },
    {
        "id": 39,
        "rusText": "После",
        "arText": "بَعْدَ"
    },
    {
        "id": 40,
        "rusText": "Возле",
        "arText": "عِنْدَ"
    },
    {
        "id": 41,
        "rusText": "Создавать, творить",
        "arText": "خَلَقَ"
    },
    {
        "id": 42,
        "rusText": "Напоследок, высказываться",
        "arText": "آخِرُ"
    },
    {
        "id": 43,
        "rusText": "Наилучший, хороший",
        "arText": "أَحْسَنُ"
    },
    {
        "id": 44,
        "rusText": "Путь",
        "arText": "سَبِيلٌ"
    },
    {
        "id": 45,
        "rusText": "Считать ложным",
        "arText": "كَذَّبَ"
    },
    {
        "id": 46,
        "rusText": "Знать, признавать",
        "arText": "عَرَفَ"
    },
    {
        "id": 47,
        "rusText": "Дело, обстоятельство",
        "arText": "أَمْرٌ"
    },
    {
        "id": 48,
        "rusText": "Воитись, осерчать",
        "arText": "غَضِبَ"
    },
    {
        "id": 49,
        "rusText": "Когда, после того как/ пока не, еще не",
        "arText": "لَمَّا"
    },
    {
        "id": 50,
        "rusText": "С, вместе",
        "arText": "مَعَ"
    },
    {
        "id": 51,
        "rusText": "Посланник",
        "arText": "رَسُولٌ"
    },
    {
        "id": 52,
        "rusText": "Мучение",
        "arText": "عَذَابٌ"
    },
    {
        "id": 53,
        "rusText": "Небо",
        "arText": "سَمَاءٌ"
    },
    {
        "id": 54,
        "rusText": "Душа",
        "arText": "نَفْسٌ"
    },
    {
        "id": 55,
        "rusText": "Скрывать, быть неведомым",
        "arText": "يَخْفَى"
    },
    {
        "id": 56,
        "rusText": "Вещь",
        "arText": "شَيْءٌ"
    },
    {
        "id": 57,
        "rusText": "Или",
        "arText": "أَوْ"
    },
    {
        "id": 58,
        "rusText": "Принести",
        "arText": "جَاءَ بِ"
    },
    {
        "id": 59,
        "rusText": "Делать, работать",
        "arText": "عَمِلَ"
    },
    {
        "id": 60,
        "rusText": "Если, если бы",
        "arText": "لَوْ"
    },
    {
        "id": 61,
        "rusText": "Этот",
        "arText": "هَذَا"
    },
    {
        "id": 62,
        "rusText": "Давать",
        "arText": "آتَى"
    },
    {
        "id": 63,
        "rusText": "Видеть",
        "arText": "رَأَى"
    },
    {
        "id": 64,
        "rusText": "Между",
        "arText": "بَيْنَ"
    },
    {
        "id": 65,
        "rusText": "Желать, хотеть",
        "arText": "يُرِيدُ"
    },
    {
        "id": 66,
        "rusText": "Те",
        "arText": "أُولَئِكَ"
    },
    {
        "id": 67,
        "rusText": "Верующий",
        "arText": "مُؤْمِنٌ"
    },
    {
        "id": 68,
        "rusText": "После",
        "arText": "بَعْدَ"
    },
    {
        "id": 69,
        "rusText": "Возле",
        "arText": "عِنْدَ"
    },
    {
        "id": 70,
        "rusText": "Создавать, творить",
        "arText": "خَلَقَ"
    },
    {
        "id": 71,
        "rusText": "Напоследок, высказываться",
        "arText": "آخِرُ"
    },
    {
        "id": 72,
        "rusText": "Наилучший, хороший",
        "arText": "أَحْسَنُ"
    },
    {
        "id": 73,
        "rusText": "Путь",
        "arText": "سَبِيلٌ"
    },
    {
        "id": 74,
        "rusText": "Считать ложным",
        "arText": "كَذَّبَ"
    },
    {
        "id": 75,
        "rusText": "Знать, признавать",
        "arText": "عَرَفَ"
    },
    {
        "id": 76,
        "rusText": "Дело, обстоятельство",
        "arText": "أَمْرٌ"
    },
    {
        "id": 77,
        "rusText": "Воитись, осерчать",
        "arText": "غَضِبَ"
    },
    {
        "id": 78,
        "rusText": "Когда, после того как/ пока не, еще не",
        "arText": "لَمَّا"
    },
    {
        "id": 79,
        "rusText": "С, вместе",
        "arText": "مَعَ"
    },
    {
        "id": 80,
        "rusText": "Те",
        "arText": "أُولَئِكَ"
    },
    {
        "id": 81,
        "rusText": "До, пока не",
        "arText": "حَتَّى"
    },
    {
        "id": 82,
        "rusText": "Хотеть, желать",
        "arText": "أَرَادَ"
    },
    {
        "id": 83,
        "rusText": "Или",
        "arText": "أَوْ"
    },
    {
        "id": 84,
        "rusText": "Моисей",
        "arText": "مُوسَىٰ"
    },
    {
        "id": 85,
        "rusText": "Следовать, придерживаться",
        "arText": "اتَّبَعَ"
    },
    {
        "id": 86,
        "rusText": "Вы",
        "arText": "أَنتُمْ"
    },
    {
        "id": 87,
        "rusText": "Сердце",
        "arText": "قَلْبٌ"
    },
    {
        "id": 88,
        "rusText": "Раб",
        "arText": "عَبْدٌ"
    },
    {
        "id": 89,
        "rusText": "Посылать, отправлять",
        "arText": "بَعَثَ"
    },
    {
        "id": 90,
        "rusText": "Но, однако",
        "arText": "لَٰكِنَّ"
    },
    {
        "id": 91,
        "rusText": "Тиран, притеснитель, несправедливый",
        "arText": "ظَالِمٌ"
    },
    {
        "id": 92,
        "rusText": "Неверующие",
        "arText": "كَافِرُونَ"
    },
    {
        "id": 93,
        "rusText": "Может быть, вероятно",
        "arText": "عَسَىٰ"
    },
    {
        "id": 94,
        "rusText": "Брать, принимать",
        "arText": "أَخَذَ"
    },
    {
        "id": 95,
        "rusText": "Семья",
        "arText": "أَهْلٌ"
    },
    {
        "id": 96,
        "rusText": "Но, наоборот",
        "arText": "بَلْ"
    },
    {
        "id": 97,
        "rusText": "Брать, принимать",
        "arText": "أَخَذَ"
    },
    {
        "id": 98,
        "rusText": "Поклоняться",
        "arText": "عَبَدَ"
    },
    {
        "id": 99,
        "rusText": "Великий",
        "arText": "عَظِيمٌ"
    },
    {
        "id": 100,
        "rusText": "Рука",
        "arText": "يَدٌ"
    },
    {
        "id": 101,
        "rusText": "Явный",
        "arText": "بَيِّنٌ"
    },
    {
        "id": 102,
        "rusText": "Милующий",
        "arText": "رَحِيمٌ"
    },
    {
        "id": 103,
        "rusText": "Знающий, свидущий",
        "arText": "عَالِمٌ"
    },
    {
        "id": 104,
        "rusText": "Некий, некоторый",
        "arText": "بَعْضٌ"
    },
    {
        "id": 105,
        "rusText": "Последний, конечный",
        "arText": "آخِرُ"
    },
    {
        "id": 106,
        "rusText": "Бог",
        "arText": "إِلَهٌ"
    },
    {
        "id": 107,
        "rusText": "Рай",
        "arText": "جَنَّةٌ"
    },
    {
        "id": 108,
        "rusText": "Кроме, иное, другое",
        "arText": "غَيْرُ"
    },
    {
        "id": 109,
        "rusText": "Кроме",
        "arText": "إِلَّا"
    },
    {
        "id": 110,
        "rusText": "Окромя",
        "arText": "سِوَى"
    },
    {
        "id": 111,
        "rusText": "Вести по правильному пути, вести, проводить",
        "arText": "هَدَى"
    },
    {
        "id": 112,
        "rusText": "Мир, мирская жизнь",
        "arText": "دُنْيَا"
    },
    {
        "id": 113,
        "rusText": "Этот",
        "arText": "هَٰذَا"
    },
    {
        "id": 114,
        "rusText": "Милость",
        "arText": "رَحْمَة"
    },
    {
        "id": 115,
        "rusText": "Послушать, принять несправедливо, притензии",
        "arText": "اِسْتَمَعَ"
    },
    {
        "id": 116,
        "rusText": "Спрашивать",
        "arText": "سَأَلَ"
    },
    {
        "id": 117,
        "rusText": "Не, никогда",
        "arText": "لَنْ"
    },
    {
        "id": 118,
        "rusText": "Находить",
        "arText": "وَجَدَ"
    },
    {
        "id": 119,
        "rusText": "Награда",
        "arText": "أَجْرٌ"
    },
    {
        "id": 120,
        "rusText": "Знание",
        "arText": "عِلْمٌ"
    },
    {
        "id": 121,
        "rusText": "Сильный, могущественный",
        "arText": "عَزِيزٌ"
    },
    {
        "id": 122,
        "rusText": "Выходить",
        "arText": "خَرَجَ"
    },
    {
        "id": 123,
        "rusText": "Мудрый",
        "arText": "حَكِيمٌ"
    },
    {
        "id": 124,
        "rusText": "Твой",
        "arText": "لَكَ"
    },
    {
        "id": 125,
        "rusText": "Есть, сделать",
        "arText": "أَكَلَ"
    },
    {
        "id": 126,
        "rusText": "Разве, ли",
        "arText": "أَلَا"
    },
    {
        "id": 127,
        "rusText": "Религия",
        "arText": "دِينٌ"
    },
    {
        "id": 128,
        "rusText": "Слово, речь, высказывание",
        "arText": "قَوْلٌ"
    },
    {
        "id": 129,
        "rusText": "Прощающий",
        "arText": "غَافِرٌ"
    },
    {
        "id": 130,
        "rusText": "Не, не имеется",
        "arText": "لَيْسَ"
    },
    {
        "id": 131,
        "rusText": "Дьявол",
        "arText": "شَيْطَانٌ"
    },
    {
        "id": 132,
        "rusText": "Делать, совершать",
        "arText": "فَعَلَ"
    },
    {
        "id": 133,
        "rusText": "Дьявол",
        "arText": "شَيْطَانٌ"
    },
    {
        "id": 134,
        "rusText": "Представать, являться",
        "arText": "حَضَرَ"
    },
    {
        "id": 135,
        "rusText": "Владеть",
        "arText": "مَلَكَ"
    },
    {
        "id": 136,
        "rusText": "Смотреть",
        "arText": "نَظَرَ"
    },
    {
        "id": 137,
        "rusText": "Склоняться, наклоняться",
        "arText": "رَكَعَ"
    },
    {
        "id": 138,
        "rusText": "Мы",
        "arText": "نَحْنُ"
    },
    {
        "id": 139,
        "rusText": "Близкий, покровитель",
        "arText": "قَرِيبٌ"
    },
    {
        "id": 140,
        "rusText": "Правильный путь",
        "arText": "صِرَاطٌ"
    },
    {
        "id": 141,
        "rusText": "Упоминать, рассказывать",
        "arText": "ذَكَرَ"
    },
    {
        "id": 142,
        "rusText": "Достоинство, честь",
        "arText": "عِزٌّ"
    },
    {
        "id": 143,
        "rusText": "Ночь",
        "arText": "لَيْلٌ"
    },
    {
        "id": 144,
        "rusText": "Бояться, страшиться",
        "arText": "خَافَ"
    },
    {
        "id": 145,
        "rusText": "Молитва",
        "arText": "صَلَاةٌ"
    },
    {
        "id": 146,
        "rusText": "Убивать, уничтожать",
        "arText": "قَتَلَ"
    },
    {
        "id": 147,
        "rusText": "Как?",
        "arText": "كَيْفَ"
    },
    {
        "id": 148,
        "rusText": "Первый",
        "arText": "أَوَّلُ"
    },
    {
        "id": 149,
        "rusText": "Ты",
        "arText": "أَنتَ"
    },
    {
        "id": 150,
        "rusText": "Наибольший, больший",
        "arText": "أَكْبَرُ"
    },
    {
        "id": 151,
        "rusText": "Сынок",
        "arText": "بُنَيَّ"
    }
]
